<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trabalhopdm">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/trab_pdm"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TrabalhoPDM">
        <activity
            android:name=".activity.SaudeActivity"
            android:exported="false" />
        <activity
            android:name=".activity.BatimentosActivity"
            android:exported="false" />
        <activity
            android:name=".activity.VacinasActivity"
            android:exported="false" />
        <activity
            android:name=".activity.PressaoActivity"
            android:exported="false" />
        <activity
            android:name=".activity.DadosPessoaisactivity"
            android:exported="false" />
        <activity
            android:name=".FirebaseControleCadastroLoginActivity"
            android:exported="false" />
        <activity
            android:name=".activity.TelaInicialActivity"
            android:exported="false" />
        <activity
            android:name=".activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".activity.CadastroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>